{
  "require": {
    "craftcms/cms": "3.3.15",
    "vlucas/phpdotenv": "^2.4.0",
    "craftcms/redactor": "^2.1",
    "ether/tags": "^1.0",
    "craftcms/contact-form": "^2.2",
    "ether/simplemap": "^3.3",
    "solspace/craft3-freeform": "^2.5",
    "solspace/craft3-freeform-pro": "^2.5",
    "barrelstrength/sprout-sitemaps": "1.0.2"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
